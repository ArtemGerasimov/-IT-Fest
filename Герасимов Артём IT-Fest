def no():
    if input('–ß—Ç–æ–±—ã –≤—ã–π—Ç–∏ –∏–∑ —Å—é–∂–µ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ "–∑–∞–≤–µ—Ä—à–∏—Ç—å", –µ—Å–ª–∏ –Ω–µ—Ç –Ω–∞–∂–º—Ç–µ "Enter"') == '–∑–∞–≤–µ—Ä—à–∏—Ç—å':
        print('–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!')
        exit()
    print('–û—á–µ–Ω—å –∂–∞–ª—å, —á—Ç–æ —Ç—ã –Ω–µ –ø–ª–∞–Ω–∏—Ä—É–µ—à—å –æ—Ç–¥—ã—Ö –≤ –¥–µ—Ç—Å–∫–æ–º –ª–∞–≥–µ—Ä–µ. –£ —Ç–µ–±—è –µ—Å—Ç—å –≤—ã–±–æ—Ä: –ø–æ–µ—Ö–∞—Ç—å –Ω–∞ –¥–∞—á—É –∏–ª–∏ –æ—Å—Ç–∞—Ç—å—Å—è '
          '–≤ –≥–æ—Ä–æ–¥–µ.')
    a = input('–í–≤–µ–¥–∏—Ç–µ –≥–æ—Ä–æ–¥ –∏–ª–∏ –¥–∞—á–∞').strip()
    while a != '–≥–æ—Ä–æ–¥' and a != '–¥–∞—á–∞':
        a = input('–û—Ç–≤–µ—Ç –Ω–µ —Ä–∞—Å–ø–æ–∑–Ω–∞–Ω, –≤–≤–µ–¥–∏—Ç–µ –æ—Ç–≤–µ—Ç –∑–∞–Ω–æ–≤–æ').strip()
    if a == '–≥–æ—Ä–æ–¥':
        print('–¢–µ–±–µ –ø—Ä–µ–¥—Å—Ç–æ–∏—Ç –≤—Å–µ –ª–µ—Ç–æ –ø—Ä–æ–≤–µ—Å—Ç–∏ –≤ –∂–∞—Ä–∫–æ–º –∏ –¥—É—à–Ω–æ–º –≥–æ—Ä–æ–¥–µ. –ñ–¥–µ–º —Ç–µ–±—è –≤ —Å–ª–µ–¥—É—é—â–µ–º –≥–æ–¥—É!')
    else:
        print('–ù—É —á—Ç–æ –∂, –ø–æ –∫—Ä–∞–π–Ω–µ–π –º–µ—Ä–µ —Ç—ã –±—É–¥–µ—à—å –Ω–∞ –ø—Ä–∏—Ä–æ–¥–µ, —ç—Ç–æ —Ç–æ–∂–µ –Ω–µ–ø–ª–æ—Ö–æ–π –æ—Ç–¥—ã—Ö. –ù–æ —Ç—ã —É–ø—É—Å—Ç–∏–ª —à–∞–Ω—Å –Ω–∞–π—Ç–∏ –Ω–æ–≤—ã—Ö')
        print('–¥—Ä—É–∑–µ–π –∏ –ø—Ä–∏–æ–±—Ä–µ—Å—Ç–∏ –Ω–æ–≤—ã–µ –∑–Ω–∞–Ω–∏—è. –ï—Å–ª–∏ –∑–∞—Ö–æ—á–µ—à—å –∏—Å–ø—ã—Ç–∞—Ç—å —Å–µ–±—è –≤ —Å–ª–µ–¥—É—é—â–µ–º –≥–æ–¥—É, –º—ã –±—É–¥–µ–º –∂–¥–∞—Ç—å —Ç–µ–±—è!')


def question():
    if input('–ß—Ç–æ–±—ã –≤—ã–π—Ç–∏ –∏–∑ —Å—é–∂–µ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ "–∑–∞–≤–µ—Ä—à–∏—Ç—å", –µ—Å–ª–∏ –Ω–µ—Ç –Ω–∞–∂–º—Ç–µ "Enter"') == '–∑–∞–≤–µ—Ä—à–∏—Ç—å':
        print('–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!')
        exit()
    print('–í —Ç–µ—á–µ–Ω–∏–∏ –¥–≤—É—Ö –Ω–µ–¥–µ–ª—å —Ç–µ–±–µ –ø—Ä–µ–¥—Å—Ç–æ–∏—Ç –∏–∑—É—á–∞—Ç—å –ª–µ–∫—Ü–∏–∏ –ø–æ –º–µ–¥–∏—Ü–∏–Ω–µ, —Ñ–∏–∑–∏–∫–µ, –∞—Ä—Ö–µ–æ–ª–æ–≥–∏–∏, —Ç–µ—Ö–Ω–∏–∫–µ, —Å–º–æ—Ç—Ä–µ—Ç—å')
    print('—É–≤–ª–µ–∫–∞—Ç–µ–ª—å–Ω—ã–µ –∏ –ø–æ–∑–Ω–∞–≤–∞—Ç–µ–ª—å–Ω—ã–µ —Å—é–∂–µ—Ç—ã –∏ –≥–æ—Ç–æ–≤–∏—Ç—å—Å—è –∫ —Ñ–∏–Ω–∞–ª—å–Ω–æ–º—É —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏—é.')
    a = input('–ù–∞–ø–∏—à–∏, –≥–æ—Ç–æ–≤ –∏–ª–∏ –Ω–µ—Ç').strip()
    while a != '–≥–æ—Ç–æ–≤' and a != '–Ω–µ—Ç':
        a = input('–û—Ç–≤–µ—Ç –Ω–µ —Ä–∞—Å–ø–æ–∑–Ω–∞–Ω, –≤–≤–µ–¥–∏—Ç–µ –æ—Ç–≤–µ—Ç –∑–∞–Ω–æ–≤–æ').strip()
    if a == '–≥–æ—Ç–æ–≤' or a == '–¥–∞':
        da()
    else:
        no()


def camp():
    if input('–ß—Ç–æ–±—ã –≤—ã–π—Ç–∏ –∏–∑ —Å—é–∂–µ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ "–∑–∞–≤–µ—Ä—à–∏—Ç—å", –µ—Å–ª–∏ –Ω–µ—Ç –Ω–∞–∂–º—Ç–µ "Enter"') == '–∑–∞–≤–µ—Ä—à–∏—Ç—å':
        print('–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!')
        exit()
    print('–í –ª–∞–≥–µ—Ä—å –º–æ–∂–Ω–æ –ø–æ–µ—Ö–∞—Ç—å –ø–æ —Ç—Ä–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è–º: —Å–ø–∞—Å–∞—Ç–µ–ª–∏, –∞—Ä—Ö–µ–æ–ª–æ–≥–∏, –≥–∏–¥—Ä–æ–Ω–∞–≤—Ç—ã.')
    print('–ù–∞ –∫–∞–∂–¥–æ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–∏ –¢–µ–±—è –∂–¥—É—Ç –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã–µ –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è, –æ—Å–æ–±–µ–Ω–Ω—ã–µ –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è.')
    print('–ù–∞–ø—Ä–∏–º–µ—Ä, —É –∞—Ä—Ö–µ–æ–ª–æ–≥–æ–≤, —ç—Ç–æ –ø–æ–∏—Å–∫–∏ —Å—Ç–∞—Ä–∏–Ω–Ω—ã—Ö –≤–µ—â–µ–π, –º–∞—Å—Ç–µ—Ä-–∫–ª–∞—Å—Å—ã –ø–æ –∏—Ö —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—é, –∏–∑—É—á–µ–Ω–∏–µ –∏—Ö –∏—Å—Ç–æ—Ä–∏–∏, –∏ –º–Ω–æ–≥–æ–µ –¥—Ä—É–≥–æ–µ.')
    print(' –£ —Å–ø–∞—Å–∞—Ç–µ–ª–µ–π –∑–∞–Ω—è—Ç–∏—è –ø–æ –≤—è–∑–∞–Ω–∏—é –º–æ—Ä—Å–∫–∏—Ö —É–∑–ª–æ–≤, –ø–æ —Å–ø—É—Å–∫—É —Å –º–æ—Å—Ç–æ–≤ –Ω–∞ –≤–æ–¥—É –ø—Ä–∏ –ø–æ–º–æ—â–∏ –∞–ª—å–ø–∏–Ω–∏—Å—Ç—Å–∫–æ–≥–æ —Å–Ω–∞—Ä—è–∂–µ–Ω–∏—è,')
    print('—Å–ø–∞—Å–µ–Ω–∏—é —É—Ç–æ–ø–∞—é—â–∏—Ö –∏ –æ–∫–∞–∑–∞–Ω–∏—é –ø–µ—Ä–≤–æ–π –ø–æ–º–æ—â–∏.')
    print('–ï—Å–ª–∏ —Ç—ã —Ä–µ—à–∏—à—å —Å—Ç–∞—Ç—å –Ω–∞ —ç—Ç–æ–π —Å–º–µ–Ω–µ –≥–∏–¥—Ä–æ–Ω–∞–≤—Ç–æ–º, —Ç–æ –∏–∑—É—á–∏—à—å —Å—Ç—Ä–æ–µ–Ω–∏–µ –±–∞—Ç–∏—Å–∫–∞—Ñ–∞ –∏ –ø–æ–ø—Ä–æ–±—É–µ—à—å –ø–æ–≥—Ä—É–∂–∞—Ç—å—Å—è –≤ –Ω–µ–º –≤ –º–æ—Ä—Å–∫–∏–µ –≥–ª—É–±–∏–Ω—ã.')
    print('–ß—Ç–æ–±—ã –≤—ã–±—Ä–∞—Ç—å –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–≤–µ–¥–∏—Ç–µ –µ–≥–æ –Ω–∞–∑–≤–∞–Ω–∏–µ: "—Å–ø–∞—Å–∞—Ç–µ–ª–∏", "–≥–∏–¥—Ä–æ–Ω–∞–≤—Ç—ã" –∏–ª–∏ —Ç—ã –º–æ–∂–µ—à—å –æ—Ç–∫–∞–∑–∞—Ç—å—Å—è –æ—Ç –ø–æ–µ–∑–¥–∫–∏ –¥–ª—è —ç—Ç–æ–≥–æ –Ω–∞–ø–∏—à–∏ "–Ω–µ –ø–æ–µ–¥—É".')
    print('–í—Å–µ —É—á–∞—Å—Ç–Ω–∏–∫–∏, —Å—Ç–∞–≤—à–∏–µ –ø–æ–±–µ–¥–∏—Ç–µ–ª—è–º–∏, –º–æ–≥—É—Ç —Å–∫–∞—á–∞—Ç—å —Å–µ—Ä—Ç–∏—Ñ–∏–∫–∞—Ç –ø–æ–±–µ–¥–∏—Ç–µ–ª—è –≤ –ª–∏—á–Ω–æ–º –∫–∞–±–∏–Ω–µ—Ç–µ.')
    a = input().strip()
    while a != '—Å–ø–∞—Å–∞—Ç–µ–ª–∏' and a != '–≥–∏–¥—Ä–æ–Ω–∞–≤—Ç—ã' and a != '–Ω–µ –ø–æ–µ–¥—É':
        print('–¢–∞–∫–æ–≥–æ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç, –≤–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è –µ—â–µ —Ä–∞–∑.')
        a = input()
    if a == '—Å–ø–∞—Å–∞—Ç–µ–ª–∏':
        print('–ñ–¥–µ–º —Ç–µ–±—è –Ω–∞ —Å–º–µ–Ω–µ –ø–æ –ø—Ä–æ—Ñ–∏–ª—é "—Å–ø–∞—Å–∞—Ç–µ–ª–∏" –¥–ª—è –ø–æ–≥—Ä—É–∂–µ–Ω–∏—è –≤ –ø—Ä–æ—Ñ–µ—Å—Å–∏—é.')
    elif a == '–≥–∏–¥—Ä–æ–Ω–∞–≤—Ç—ã':
        print('–ñ–¥–µ–º —Ç–µ–±—è –Ω–∞ —Å–º–µ–Ω–µ –ø–æ –ø—Ä–æ—Ñ–∏–ª—é "–≥–∏–¥—Ä–æ–Ω–∞–≤—Ç—ã" –¥–ª—è –∏–∑—É—á–µ–Ω–∏—è –º–æ—Ä—Å–∫–∏ –≥–ª—É–±–∏–Ω.')
    elif a == '–Ω–µ –ø–æ–µ–¥—É':
        print('–û—á–µ–Ω—å –∂–∞–ª—å, —á—Ç–æ —Ç—ã –Ω–µ —Å–º–æ–∂–µ—à—å –ø—Ä–∏—Å–æ–µ–¥–∏–Ω–∏—Ç—å—Å—è –∫ –Ω–∞–º –Ω–∞ —Å–º–µ–Ω–µ –≤ –ª—É—á—à–µ–º –¥–µ—Ç—Å–∫–æ–º –ª–∞–≥–µ—Ä–µ "–ß–∞–π–∫–∞". –ñ–¥–µ–º —Ç–µ–±—è –Ω–∞ —Å–ª–µ–¥—É—é—â–∏–π –≥–æ–¥.')
        no()


def test():
    if input('–ß—Ç–æ–±—ã –≤—ã–π—Ç–∏ –∏–∑ —Å—é–∂–µ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ "–∑–∞–≤–µ—Ä—à–∏—Ç—å", –µ—Å–ª–∏ –Ω–µ—Ç –Ω–∞–∂–º–∏—Ç–µ "Enter"') == '–∑–∞–≤–µ—Ä—à–∏—Ç—å':
        print('–î–æ —Å–≤–∏–¥–∞–Ω–∏—è, –æ—á–µ–Ω—å –∂–∞–ª—å, —á—Ç–æ —Ç—ã —Ä–µ—à–∏–ª –∑–∞–≤–µ—Ä—à–∏—Ç—å –æ–±—É—á–µ–Ω–∏–µ.')
        exit()
    print('–î–ª—è –ø–æ–µ–∑–¥–∫–∏ –≤ –ª–∞–≥–µ—Ä—å –≤–∞–º –Ω—É–∂–Ω–æ –ø—Ä–æ–π—Ç–∏ —Ñ–∏–Ω–∞–ª—å–Ω—ã–π —ç—Ç–∞–ø.')
    print('–£ –≤–∞—Å –µ—Å—Ç—å –æ–¥–Ω–∞ –ø–æ–ø—ã—Ç–∫–∞ —á—Ç–æ–±—ã –ø—Ä–∞–≤–∏–ª—å–Ω–æ –æ—Ç–≤–µ—Ç–∏—Ç—å –Ω–∞ –≤–æ–ø—Ä–æ—Å')
    print('–í–≤–µ–¥–∏—Ç–µ –æ—Ç–≤–µ—Ç —Å—Ç—Ä–æ—á–Ω—ã–º–∏ –±—É–∫–≤–∞–º–∏ –≤ –∏–º–µ–Ω–∏—Ç–µ–ª—å–Ω–æ–º –ø–∞–¥–µ–∂–µ, –µ–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω–æ–º —á–∏—Å–ª–µ.')
    print('–í–æ–ø—Ä–æ—Å: –ß–µ–≥–æ –±–æ–ª—å—à–µ –Ω–∞ –ø–ª–∞–Ω–µ—Ç–µ –ó–µ–º–ª—è?')
    a = input()
    if a == '–≤–æ–¥–∞':
        print('–ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º! –¢—ã –±—ã–ª –æ—á–µ–Ω—å –≤–Ω–∏–º–∞—Ç–µ–ª–µ–Ω, –ø—Ä–µ–æ–¥–æ–ª–µ–ª –º–Ω–æ–≥–æ —Ç—Ä—É–¥–Ω–æ—Å—Ç–µ–π, —É–∑–Ω–∞–ª –º–Ω–æ–≥–æ –Ω–æ–≤–æ–≥–æ, –æ—Ç–≤–µ—Ç–∏–ª –Ω–∞ –≤—Å–µ')
        print('–Ω–∞—à–∏ –≤–æ–ø—Ä–æ—Å—ã –∏ –≤—ã–∏–≥—Ä–∞–ª –ø–æ–µ–∑–¥–∫—É –≤ –æ–¥–∏–Ω –∏–∑ –ª—É—á—à–∏—Ö –ª–∞–≥–µ—Ä–µ–π —Å—Ç—Ä–∞–Ω—ã!!! –ú—ã –ø–æ–∑–¥—Ä–∞–≤–ª—è–µ–º —Ç–µ–±—è –∏')
        print('–∂–µ–ª–∞–µ–º –æ—Ç–ª–∏—á–Ω–æ–≥–æ –æ—Ç–¥—ã—Ö–∞ –Ω–æ–≤—ã—Ö –ø–æ–±–µ–¥!. üéâüî•üí™')
        camp()
    else:
        no()


def da():
    if input('–ß—Ç–æ–±—ã –≤—ã–π—Ç–∏ –∏–∑ —Å—é–∂–µ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ "–∑–∞–≤–µ—Ä—à–∏—Ç—å", –µ—Å–ª–∏ –Ω–µ—Ç –Ω–∞–∂–º–∏—Ç–µ "Enter"') == '–∑–∞–≤–µ—Ä—à–∏—Ç—å':
        print('–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!')
        exit()
    print('–¢—ã –≤–Ω–∏–º–∞—Ç–µ–ª—å–Ω–æ –∏–∑—É—á–∏–ª –≤–µ—Å—å –º–∞—Ç–µ—Ä–∏–∞–ª, –ø—Ä–∏—à–ª–æ –≤—Ä–µ–º—è –ø—Ä–æ–π—Ç–∏ —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ. –í–æ—Ç –ø–µ—Ä–≤—ã–π –≤–æ–ø—Ä–æ—Å.')
    print(
        '–ö–∞–∫ —Ç—ã —Å—á–∏—Ç–∞–µ—à—å, –≤–µ—Ä–Ω–æ –ª–∏ —Å–ª–µ–¥—É—é—â–µ–µ —É—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ: –ù–∞–≤–æ–¥–Ω–µ–Ω–∏–µ - –±–µ–¥—Å—Ç–≤–∏–µ, –≤–æ –≤—Ä–µ–º—è –∫–æ—Ç–æ—Ä–æ–≥–æ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –∫–∞–∫ ')
    print('–º–æ–∂–Ω–æ –±—ã—Å—Ç—Ä–µ–µ –ø–æ–∫–∏–Ω—É—Ç—å –Ω–∏–∑–∏–Ω–Ω—ã–µ –æ–±–ª–∞—Å—Ç–∏ –∏ –¥–æ–±—Ä–∞—Ç—å—Å—è –¥–æ –Ω–∞–∏–±–æ–ª–µ–µ –≤–æ–∑–≤—ã—à–µ–Ω–Ω–æ–π —Ç–æ—á–∫–∏ —Ä–µ–ª—å–µ—Ñ–∞?')
    a = input('–¥–∞ –∏–ª–∏ –Ω–µ—Ç').strip()
    while a != '–¥–∞' and a != '–Ω–µ—Ç':
        a = input('–û—Ç–≤–µ—Ç –Ω–µ —Ä–∞—Å–ø–æ–∑–Ω–∞–Ω, –≤–≤–µ–¥–∏—Ç–µ –æ—Ç–≤–µ—Ç –∑–∞–Ω–æ–≤–æ').strip()
    if a == '–¥–∞':
        print('–ù–∞ –ø–µ—Ä–≤—ã–π –≤–æ–ø—Ä–æ—Å —Ç—ã –æ—Ç–≤–µ—Ç–∏–ª –≤–µ—Ä–Ω–æ. –ú–æ–ª–æ–¥–µ—Ü, –∑–Ω–∞—á–∏—Ç —Ç—ã –≤–Ω–∏–º–∞—Ç–µ–ª—å–Ω–æ –∏–∑—É—á–∞–ª –º–∞—Ç–µ—Ä–∏–∞–ª! –ß—Ç–æ–±—ã –æ—Ç–≤–µ—Ç–∏—Ç—å')
        print('–Ω–∞ —Å–ª–µ–¥—É—é—â–∏–π –≤–æ–ø—Ä–æ—Å —É —Ç–µ–±—è –±—É–¥–µ—Ç 4 –ø–æ–ø—ã—Ç–∫–∏.')
        print('–ï—Å–ª–∏ —Ç—ã –≤–µ—Ä–Ω–æ –æ—Ç–≤–µ—Ç–∏—à—å –Ω–∞ –≤–æ–ø—Ä–æ—Å, –ø—Ä–æ–π–¥–µ—à—å –≤ —Å–ª–µ–¥—É—é—â–∏–π —ç—Ç–∞–ø.')
        print('–î–ª—è –æ—Ç–≤–µ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ —Ü–∏—Ñ—Ä—ã –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ –±–µ–∑ –ø—Ä–æ–±–µ–ª–æ–≤')
        print('–í–æ–ø—Ä–æ—Å: –ö–∞–∫ –Ω–∞–∑—ã–≤–∞—é—Ç—Å—è —Å–∞–º—ã–π —Å–µ–≤–µ—Ä–Ω—ã–π –∏ —Å–∞–º—ã–π —é–∂–Ω—ã–π –æ–∫–µ–∞–Ω.')
        print('–í–∞—Ä–∏–∞–Ω—Ç—ã –æ—Ç–≤–µ—Ç–æ–≤:')
        print('1. –°–µ–≤–µ—Ä–Ω—ã–π –ª–µ–¥–æ–≤–∏—Ç—ã–π –æ–∫–µ–∞–Ω')
        print('2. –¢–∏—Ö–∏–π –æ–∫–µ–∞–Ω')
        print('3. –ò–Ω–¥–∏–π—Å–∫–∏–π –æ–∫–µ–∞–Ω')
        print('4. –Æ–∂–Ω—ã–π –æ–∫–µ–∞–Ω')
        a = input().strip()
        k = 0
        f = 'False'
        while f == 'False' and k < 3:
            if a != '14' and a != '41':
                a = input('–û—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π, –≤–≤–µ–¥–∏—Ç–µ –¥—Ä—É–≥–æ–π –≤–∞—Ä–∏–∞–Ω—Ç')
                k += 1
            elif a == '41' or a == '14':
                f = 'True'
        if f == 'True':
            print('–ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º! –¢—ã –ø—Ä–æ—à–µ–ª –æ—Ç–±–æ—Ä–æ—á–Ω—ã–π —ç—Ç–∞–ø! –¢–µ–ø–µ—Ä—å —Ç–µ–±–µ –ø—Ä–µ–¥—Å—Ç–æ–∏—Ç –ø—Ä–æ–π—Ç–∏ —Å–∞–º—ã–π —Å–ª–æ–∂–Ω—ã–π —Ñ–∏–Ω–∞–ª—å–Ω—ã–π —ç—Ç–∞–ø, —á—Ç–æ–±—ã –ø–æ–µ—Ö–∞—Ç—å –≤ –ª–∞–≥–µ—Ä—åüëç')
            test()
        else:
            print(
                '–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —É —Ç–µ–±—è –∑–∞–∫–æ–Ω—á–∏–ª–∏—Å—å –≤—Å–µ –ø–æ–ø—ã—Ç–∫–∏. –ù–µ —Ä–∞—Å—Å—Ç—Ä–∞–∏–≤–∞–π—Å—è, —Ç—ã –º–æ–∂–µ—Ç–µ –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å —Å–≤–æ–∏ —Å–∏–ª—ã –≤ —Å–ª–µ–¥—É—é—â–µ–º –≥–æ–¥—É.')
            no()


print('–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π, –¥—Ä—É–≥! –£–∂–µ —Å–∫–æ—Ä–æ –ª–µ—Ç–æ. –ú—ã –¥—É–º–∞–µ–º, —á—Ç–æ –≤–µ—Å—å –≥–æ–¥ —Ç—ã —Ö–æ—Ä–æ—à–æ —É—á–∏–ª—Å—è –≤ —à–∫–æ–ª–µ, –∑–∞–Ω–∏–º–∞–ª—Å—è –≤ –ö–≤–∞–Ω—Ç–æ—Ä–∏—É–º–µ, —É—á–∞—Å—Ç–≤–æ–≤–∞–ª')
print('–≤ –æ–ª–∏–º–ø–∏–∞–¥–∞—Ö –∏ –∫–æ–Ω–∫—É—Ä—Å–∞—Ö, —Å–ø–æ—Ä—Ç–∏–≤–Ω—ã—Ö —Å–æ—Ä–µ–≤–Ω–æ–≤–∞–Ω–∏—è—Ö. –ê —Å–µ–π—á–∞—Å —É —Ç–µ–±—è –µ—Å—Ç—å –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å –ø—Ä–∏–Ω—è—Ç—å —É—á–∞—Å—Ç–∏–µ –≤ –∫–æ–Ω–∫—É—Ä—Å–µ')
print('–∏ –≤—ã–∏–≥—Ä–∞—Ç—å –ø–æ–µ–∑–¥–∫—É –≤ –¥–µ—Ç—Å–∫–∏–π –æ–∑–¥–æ—Ä–æ–≤–∏—Ç–µ–ª—å–Ω—ã–π –ª–∞–≥–µ—Ä—å "–ß–∞–π–∫–∞", —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–Ω—ã–π –Ω–∞ –±–µ—Ä–µ–≥—É –ß–µ—Ä–Ω–æ–≥–æ –º–æ—Ä—è!')
print('–•–æ—á–µ—à—å –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å —Å–≤–æ–∏ —Å–∏–ª—ã –∏ —Å—Ç–∞—Ç—å —Å—á–∞—Å—Ç–ª–∏–≤—ã–º –æ–±–ª–∞–¥–∞—Ç–µ–ª–µ–º –∑–∞–≤–µ—Ç–Ω–æ–π –ø—É—Ç–µ–≤–∫–∏?')
a = input('–°–∫–æ—Ä–µ–π –≤—ã–±–∏—Ä–∞–π –¥–∞ –∏–ª–∏ –Ω–µ—Ç!').strip()
while a != '–¥–∞' and a != '–Ω–µ—Ç':
    a = input('–û—Ç–≤–µ—Ç –Ω–µ —Ä–∞—Å–ø–æ–∑–Ω–∞–Ω, –≤–≤–µ–¥–∏—Ç–µ –æ—Ç–≤–µ—Ç –∑–∞–Ω–æ–≤–æ').strip()
if a == '–¥–∞':
    question()
elif a == '–Ω–µ—Ç':
    no()
